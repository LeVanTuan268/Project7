<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE asul SYSTEM "http://www.megazine3.de/asul.dtd">
<asul>
	
	<box id="passwordform" background="color(0x333333)" anchors="0,0,pw,ph" mouseenabled="true">
		<box width="120" height="50" style="container" anchors="(pw-w)/2,(ph-h)/2" padding="5,5,5,5">
			<text anchors="0,0,pw-10,25" value="localize(LNG_PASSWORD, Password)" style="info title"/>
			<box anchors="0,19,pw-10,ph-10" style="input">
				<box name="error_fader" anchors="0,0,pw,ph" background="color(0xFF0000)" blendmode="overlay"/>
				<text name="txt_password" password="true" align="center"/>
			</box>
		</box>
	</box>
	
</asul>
